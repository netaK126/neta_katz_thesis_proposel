@inproceedings{overapprox,
author = {Anderson, Greg and Pailoor, Shankara and Dillig, Isil and Chaudhuri, Swarat},
title = {Optimization and Abstraction: A Synergistic Approach for Analyzing Neural Network Robustness},
year = {2019},
isbn = {9781450367127},
